<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <Authors>Thiago Barradas</Authors>
    <Company>Thiago Barradas</Company>
    <Description>Automatic log communication (request and response) from RestSharp using Serilog.</Description>
    <PackageProjectUrl>https://github.com/ThiagoBarradas/restsharp-serilog-auto</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ThiagoBarradas/restsharp-serilog-auto</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="RestSharp" Version="106.3.1" />
    <PackageReference Include="Serilog" Version="2.7.1" />
    <PackageReference Include="Serilog.Extensions.Logging" Version="2.0.2" />
    <PackageReference Include="Serilog.Settings.Configuration" Version="2.6.1" />
    <PackageReference Include="Serilog.Sinks.Console" Version="3.1.2-dev-00777" />
  </ItemGroup>

</Project>
