<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Yaseer Arafat</Authors>
    <Company>Yaseer Arafat</Company>
    <Description>Automatic log communication (request and response) from RestSharp using Serilog.
      Extendd from Thiago Barradas</Description>
    <PackageProjectUrl>https://github.com/emonarafat/restsharp-serilog-auto</PackageProjectUrl>
    <RepositoryUrl>https://github.com/emonarafat/restsharp-serilog-auto</RepositoryUrl>
    <Copyright>â’¸ Yaseer Arafat</Copyright>
    <PackageIconUrl>https://i.imgur.com/GkciSHC.png</PackageIconUrl>
    <OutputType>Library</OutputType>
    <StartupObject />
    <PackageTags>restsharp, serilog, autolog, log, seq, splunk, console, sink, core, framework, standard</PackageTags>
    <Version>1.0.1</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="JsonMasking" Version="1.1.3" />
